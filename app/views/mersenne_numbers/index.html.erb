<p id="notice"><%= notice %></p>

<h1>Mersenne Numbers</h1>

<table>
  <thead>
    <tr>
      <th>Max number</th>
      <th>Numbers</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mersenne_numbers.each do |mersenne_number| %>
      <tr>
        <td><%= mersenne_number.max_number %></td>
        <td><%= mersenne_number.numbers %></td>
        <td><%= link_to 'Show', mersenne_number %></td>
        <td><%= link_to 'Edit', edit_mersenne_number_path(mersenne_number) %></td>
        <td><%= link_to 'Destroy', mersenne_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mersenne Number', new_mersenne_number_path %>
